
function start(){
    while (facingSouth()) {
        turnAround();



    }



    while (facingWest()) {



        turnRight();



    }



    while (facingEast()) {



        turnLeft();



    }   



    while (frontIsClear()) {



        putBall();


        move();



    }



    if (noBallsPresent()) {



        putBall();



    }



}



function a(){



    move();
}


function b(){



    move();



}



function c(){



    move();



}
