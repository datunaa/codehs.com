/* This program will have Karel run around the racetrack
 * 8 times. */
function start() {
    for(var i = 0 ; i < 32 ; i++) {
        run();
        putBallTurn();
    }
}
function run() {
    while(frontIsClear()) {
        move();
    }
}
function putBallTurn() {
    putBall();
    turnLeft();
}
